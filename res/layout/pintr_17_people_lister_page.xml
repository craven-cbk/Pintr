<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pintr_v_0_01.PintrMain" >

<LinearLayout
	android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:orientation="vertical"
    style="@style/generalPageBackground">


    <include layout="@layout/pintr_g_99_header_view"/>
	
	<ScrollView
		android:id="@+id/profileListSearchPage"
		android:layout_width="fill_parent"
	    android:layout_height="fill_parent" >
		   
		<LinearLayout
			android:layout_marginTop="10dp"
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
			android:paddingLeft="10dp"
			android:paddingRight="20dp"
		    android:orientation="vertical" >
		    
		   
		    
		    <LinearLayout
				android:id="@+id/profileListResultSection"
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
				android:gravity="left"
				android:paddingTop="20dp"
				android:paddingBottom="20dp"
				android:paddingLeft="30dp"
				android:paddingRight="30dp"
			    android:orientation="vertical" >
			   
				
			</LinearLayout>
		   
		    
		</LinearLayout>
		
	</ScrollView>


</LinearLayout>



<include layout="@layout/pintr_g_96_settings_menu_view"/>
    
    
</RelativeLayout>