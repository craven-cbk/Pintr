<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pintr_v_0_01.PintrMain" >

<LinearLayout
	android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:orientation="vertical"
    style="@style/generalPageBackground">

   
    <include layout="@layout/pintr_g_99_header_view"/>
  
	
	<ScrollView
		android:id="@+id/interestsListPage"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent" >
		   
		<LinearLayout
	    	android:id="@+id/GeneralDisplayArea" 
		    android:layout_width="match_parent"
		    android:layout_height="wrap_content"
			android:paddingLeft="10dp"
			android:paddingRight="20dp"
		    android:orientation="vertical"
		    android:paddingBottom="@dimen/activity_vertical_margin"
		    android:paddingTop="@dimen/activity_vertical_margin" >
		     
			<LinearLayout
		    	android:id="@+id/InterestsDisplayArea" 
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
				android:paddingLeft="10dp"
				android:paddingRight="20dp"
			    android:orientation="vertical"
			    android:paddingBottom="@dimen/activity_vertical_margin"
			    android:paddingTop="@dimen/activity_vertical_margin" >
			   
			</LinearLayout>
		 
			<LinearLayout
		    	android:id="@+id/ButtonDisplayArea" 
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
				android:paddingLeft="10dp"
				android:paddingRight="20dp"
			    android:orientation="vertical"
			    android:gravity="center"
			    android:paddingBottom="@dimen/activity_vertical_margin"
			    android:paddingTop="@dimen/activity_vertical_margin" >
		
				 <Button
			        android:id="@+id/BackToInterestsButton"
			        android:background="@drawable/general_button" 
			        style="@style/ButtonText"
					android:paddingLeft="15dp"
					android:paddingRight="15dp"
			        android:layout_marginTop="20dp"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="Back to Interests" />	   
			</LinearLayout>
		
		
		</LinearLayout>
	</ScrollView>


</LinearLayout>



<include layout="@layout/pintr_g_96_settings_menu_view"/>
    
    
</RelativeLayout>
